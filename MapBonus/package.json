{
  "name": "map_view",
  "version": "1.0.0",
  "description": "Map of drivers for P4.3",
  "main": "src/server.js",
  "scripts": {
    "start": "npm run build && babel-node src/server.js",
    "start:dev": "export NODE_ENV=development && npm run build:dev && nodemon --exec babel-node -- src/server.js",
    "start:universal": "export UNIVERSAL=true && npm run start",
    "start:dev:universal": "export NODE_ENV=development && export UNIVERSAL=true && npm run start:dev",
    "build": "NODE_ENV=production webpack --define process.env.NODE_ENV=\"production\"",
    "build:dev": "webpack -d",
    "build:dev:watch": "webpack -d --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autobind-decorator": "^2.1.0",
    "babel-polyfill": "^6.23.0",
    "babel-register": "^6.24.0",
    "d3-color": "^1.1.0",
    "d3-request": "^1.0.5",
    "deck.gl": "5.1.4",
    "ejs": "^2.5.9",
    "express": "^4.15.2",
    "immutable": "^3.8.1",
    "kafka-node": "^2.5.0",
    "luma.gl": "5.1.6",
    "npm-check-updates": "^2.13.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-map-gl": "^3.2.7",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.1.1",
    "redux": "^4.0.0",
    "redux-actions": "^2.0.1",
    "redux-thunk": "^2.2.0",
    "stats.js": "^0.17.0",
    "tween.js": "^16.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-static-fs": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^4.12.1",
    "eslint-plugin-react": "^7.5.1",
    "nodemon": "^1.11.0",
    "transform-loader": "^0.2.4",
    "webpack": "4.19.1",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.10"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "rules": {
      "guard-for-in": 0,
      "camelcase": 0
    },
    "globals": {
      "MAPBOX_ACCESS_TOKEN": false
    }
  }
}
